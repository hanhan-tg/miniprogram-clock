/* pages/my/index.wxss */
.my-container {
  margin: 0 auto;
}

.content {
  background-color: #fff;
  margin-top: 20rpx;
  border-radius: 30rpx;
  padding: 0 30rpx;
}

.item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 150rpx;
  border-bottom: 1px solid rgba(0, 0, 0, .05);
}

.left {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-left: 30rpx;
}

.text {
  margin-left: 30rpx;
}